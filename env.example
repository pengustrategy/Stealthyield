# Stealthyield Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env file to git!

# Wallet Paths (store in secure location OUTSIDE project)
DEPLOYER_WALLET_PATH=/secure/path/to/deployer-wallet.json
MOTHERWOMB_WALLET_PATH=/secure/path/to/motherwomb-wallet.json

# Network Configuration (update with your Helius API key)
RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY
NETWORK=mainnet-beta

# Token Info (will be filled after creation)
STYD_MINT=
RAYDIUM_POOL=

# Distribution Settings
EMISSION_INTERVAL=600000
AUTO_PROCESS_INTERVAL=600000

